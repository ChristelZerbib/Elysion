{% extends 'base.html.twig' %}

{% block title %}GlyphsList{% endblock %}

{% block body %}
    <h1>GlyphsList</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ glyphs_list.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ glyphs_list.name }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ glyphs_list.price }}</td>
            </tr>
            <tr>
                <th>Isunique</th>
                <td>{{ glyphs_list.isunique ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ glyphs_list.description }}</td>
            </tr>
            <tr>
                <th>Bought</th>
                <td>{{ glyphs_list.bought ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Used</th>
                <td>{{ glyphs_list.used ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Characters</th>
                <td>{{ glyphs_list.characters }}</td>
            </tr>
            <tr>
                <th>Effect</th>
                <td>{{ glyphs_list.effect }}</td>
            </tr>
            <tr>
                <th>Support</th>
                <td>{{ glyphs_list.support }}</td>
            </tr>
            <tr>
                <th>Special</th>
                <td>{{ glyphs_list.special ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Support_2</th>
                <td>{{ glyphs_list.support2 }}</td>
            </tr>
            <tr>
                <th>Support_3</th>
                <td>{{ glyphs_list.support3 }}</td>
            </tr>
            <tr>
                <th>Evol_salary</th>
                <td>{{ glyphs_list.evolSalary }}</td>
            </tr>
            <tr>
                <th>Evol_maintenance</th>
                <td>{{ glyphs_list.evolMaintenance }}</td>
            </tr>
            <tr>
                <th>Evol_staff</th>
                <td>{{ glyphs_list.evolStaff }}</td>
            </tr>
            <tr>
                <th>Personalization</th>
                <td>{{ glyphs_list.personalization ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('glyphs_list_index') }}">back to list</a>

    <a href="{{ path('glyphs_list_edit', {'id': glyphs_list.id}) }}">edit</a>

    {{ include('glyphs_list/_delete_form.html.twig') }}
{% endblock %}
