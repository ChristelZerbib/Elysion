<form method="post" action="{{ path('objects_list_delete', {'id': objects_list.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer cet objet ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ objects_list.id) }}">
    <button class="btn btn-dark text-info border-info btn-block">Supprimer</button>
</form>
