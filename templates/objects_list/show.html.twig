{% extends 'base.html.twig' %}

{% block title %}ObjectsList{% endblock %}

{% block body %}
    <h1>ObjectsList</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ objects_list.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ objects_list.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ objects_list.description }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ objects_list.comment }}</td>
            </tr>
            <tr>
                <th>Shop</th>
                <td>{{ objects_list.shop }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ objects_list.price }}</td>
            </tr>
            <tr>
                <th>Max_number</th>
                <td>{{ objects_list.maxNumber }}</td>
            </tr>
            <tr>
                <th>Personalization</th>
                <td>{{ objects_list.personalization ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('objects_list_index') }}">back to list</a>

    <a href="{{ path('objects_list_edit', {'id': objects_list.id}) }}">edit</a>

    {{ include('objects_list/_delete_form.html.twig') }}
{% endblock %}
